# Global snippets

snippet fun
	fun ${1}(${2}: ${3}): ${4} {
		${5}
	}
snippet val
	val ${1} = ${2}
snippet var
	var ${1} = ${2}
snippet as
	assert ${1:test} : "${2:Failure message}";${3}
snippet try
	try {
		${3}
	} catch(${1:Exception} ${2:e}) {
	}
snippet tryf
	try {
		${3}
	} catch(${1:Exception} ${2:e}) {
	} finally {
	}




snippet main
	fun main(args : Array<String>) {
		${1}
	}
snippet if
	if (${1}) {
		${2}
	}
snippet el
	else {
		${1}
	}
snippet do
