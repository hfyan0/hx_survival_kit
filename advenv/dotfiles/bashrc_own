unset -f print_dir_info; print_dir_info() {
    local YELLOW="\033[0;33m"
    local YELLOWBOLD="\033[1;33m"
    local WHITE="\033[0;37m"
    local NOCOLOR="\033[0m"

    echo
    pwd
    echo "--------------------------------------------------"

    if [[ $(ls | wc -l) -lt 100 ]]
    then
        ls --color=auto --group-directories-first
    else
        ls --group-directories-first
    fi

    echo -e -n "$YELLOW"
    if [[ $(ls -a | wc -l) -eq 2 ]]
    then
        echo "Folder is empty"
    elif [[ $(ls | wc -l) -eq 0 ]]
    then
        echo "Folder only contains hidden files"
    fi
    echo -e -n "\033[00m"

}
unset -f c; c() {
    ##################################################
    # cd without any argument
    ##################################################
    if [[ $# -eq 0 ]]
    then
        if [[ $(ls | wc -l) -eq 1 && -d $(ls) ]]
        then
            ##################################################
            # cd into that one and only one directory
            ##################################################
            cd $(ls)
            print_dir_info
        else
            cd
            print_dir_info
        fi
    ##################################################
    # cd into directory if it exists
    ##################################################
    elif [[ -d $1 ]]
    then
        cd "$1" && print_dir_info
    ##################################################
    # cd to the directory containing the file
    ##################################################
    elif [[ -f $1 ]]
    then
        cd "$(dirname $1)"
        print_dir_info
    ##################################################
    # cd directory
    ##################################################
    else
        tmpfile=$(mktemp)
        bash -c "{ find -L -path './*' -prune -type d | grep $1; ls -F | grep / | grep $1; } | head -n 1" > $tmpfile
        ##################################################
        # search on the top layer first
        ##################################################
        if [[ -n $(cat $tmpfile) ]]
        then
            cd "$(cat $tmpfile)" && print_dir_info
        else
            cd "$(dirname $1)" && print_dir_info
        fi
        rm -f $tmpfile
    fi
}
unset -f i; i() {
    cd $(echo $(ls -Fd ../*$1* | grep "/$") | awk '{print $1}') && print_dir_info
}
unset -f rf; rf() {
    if [[ $@ == "." ]]
    then
        CUR_DIR=$(pwd)
        cd ..
        rm -rf $CUR_DIR
    else
        rm -rf $@
    fi
    ls --color=auto --group-directories-first
}
unset -f rd; rd() {
    if [[ $@ == "." ]]
    then
        CUR_DIR=$(pwd)
        cd ..
        rmdir $CUR_DIR
    else
        rmdir $@
    fi
    ls --color=auto --group-directories-first
}

unset -f ff; ff() {
  if [[ $# -eq 0 ]]
  then
      find . -type f
  else
      tmpfile1=$(mktemp)
      tmpfile2=$(mktemp)
      find . -type f > $tmpfile1
      for a in $@
      do
          cat $tmpfile1 | grep $a > $tmpfile2
          cat $tmpfile2 > $tmpfile1
      done
      cat $tmpfile1
      rm -f $tmpfile1
      rm -f $tmpfile2
  fi
}
unset -f gg; gg() {
    grep -B 1 -A 1 --color=auto "$@" . -r
}

unset -f gv; gv() {
    tmpfile1=$(mktemp)
    tmpfile2=$(mktemp)
    cat - > $tmpfile1
    for x in $@
    do
        cat $tmpfile1 | grep -v $x > $tmpfile2
        cat $tmpfile2 > $tmpfile1
    done
    cat $tmpfile1
    rm -f $tmpfile1 $tmpfile2
}
unset -f gsc; gsc() {
    if [[ -e $(which rg) ]]
    then
        find . -type f ! -name '*.*' | xargs grep --color=auto $@
        rg -tpy -tc -tcpp -tconfig -tjs -tjava -thtml -txml -tphp -tcs -tkotlin -tjson -tyaml -tsql -tcmake -tmake -tmd -tsh $@
    else
        find . -type f | tr "\n" "\0" | xargs -0 grep --color=auto $@
    fi
}
unset -f mvb; mvb(){
    if [[ -e $1 ]]
    then
        if [[ $1 == *"_bk_20"* ]]
        then
            ori_name=$(echo $1 | sed -e 's/_bk_20.*$//')
            mv $1 $ori_name
        else
            local thisdir=$(pwd)
            local fldr=$(dirname $1)
            local bnm=$(basename $1)
            cd $fldr
            mv $bnm $bnm"_"$(date +'%Y%m%d_%H%M%S')
            ls --color=auto --group-directories-first
            cd $thisdir
        fi
    fi
}
unset -f cpb; cpb(){
    if [[ -e $1 ]]
    then
        local thisdir=$(pwd)
        local fldr=$(dirname $1)
        local bnm=$(basename $1)
        cd $fldr
        newname=$bnm"_"$(date +'%Y%m%d_%H%M%S')
        cp -rp $bnm $newname
        chmod a-wx $newname
        echo "===================="
        echo "ls $fldr"
        echo "===================="
        ls --color=auto --group-directories-first $fldr
        cd $thisdir
    fi
}
unset -f h; h() {
    if [[ $# -eq 0 ]]
    then
        bash -c "awk '{print}' | head -n 1"
    elif [[ $# -gt 3 ]]
    then
        bash -c "head $@"
    elif [[ -f "$1" ]]
    then
        bash -c "head -n 1 $1"
    elif [[ ! -f "$1" && -f "$2" ]]
    then
        bash -c "head -n $1 $2"
    else
        bash -c "head -n $@"
    fi
}
unset -f t; t() {
    if [[ $# -eq 0 ]]
    then
        bash -c "awk '{print}' | tail -n 1"
    elif [[ $# -gt 3 ]]
    then
        bash -c "tail $@"
    elif [[ -f "$1" ]]
    then
        bash -c "tail -n 1 $1"
    elif [[ ! -f "$1" && -f "$2" ]]
    then
        bash -c "tail -n $1 $2"
    else
        bash -c "tail -n $@"
    fi
}
unset -f ty; ty() {
    st_tmpfile1=~/.advenv/tmp/stmp_$(whoami)_1
    tmpfile1=$(mktemp)
    python3 ~/.advenv/bin/typing_console2.py $tmpfile1
    echo >> $st_tmpfile1
    cat $tmpfile1 >> $st_tmpfile1
    cat $st_tmpfile1 | sed -e 's/@@/\n/g'
    echo
    echo
    rm -f $tmpfile1
}

unset -f xsi; xsi() {
    xs_base 2 $@
}
unset -f xs; xs() {
    xs_base 1 $@
}
unset -f xs_base; xs_base() {
    if [[ $1 -eq 1 ]]
    then
        local GOPT="-r -B 3 -A 5 --color=always"
    elif [[ $1 -eq 2 ]]
    then
        local GOPT="-i -r -B 3 -A 5 --color=always"
    fi
    argm=${@:2:999}

    if [[ -n $argm ]]
    then
        {
            cd ~/sunny/
                grep "$argm" \
                *.org \
                *.txt \
                $GOPT; \
            cd ~/sunny/doc; \
                grep "$argm" \
                *.txt \
                $GOPT; \
            cd ~/sunny/doc/si_2024; \
                grep "$argm" \
                *.txt \
                $GOPT; \
            cd ~/sunny/doc/si_2025; \
                grep "$argm" \
                *.txt \
                $GOPT; \
        } | \
        sed -e 's/.txt//' | \
        sed -e 's/.org//' | \
        grep -v "dat_202" | \
        less -R
    fi
}

