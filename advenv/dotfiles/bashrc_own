unset -f gtag; gtag() {
    TAG=$1
    HASH=$2
    echo "TAG: $TAG"
    echo "HASH: $HASH"
    echo "Correct? [ctrl-C to abort]"
    read
    git tag $1 $2
    git push origin $1
}
unset -f gtagd; gtagd() {
    git tag -d $@
    git push origin $@
}
