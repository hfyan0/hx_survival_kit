unset -f grbih; grbih() {
    STASHED_FIRST=0
    if [[ -n $(git stash list) ]]
    then
        return
    fi
    if [[ -n $(git status -sb | grep "^ M ") && -z $(git stash list) ]]
    then
        git stash
        STASHED_FIRST=1
    fi
    if [[ $# -eq 0 ]]
    then
        no_of_commits=$(git log --oneline | wc -l)
        if [[ $no_of_commits -gt 10 ]]
        then
            git rebase -i HEAD~10
        else
            git rebase -i HEAD~$((${no_of_commits}-1))
        fi
    elif [[ $1 == "0" || $1 == "r" ]]
    then
        git rebase -i --root
    else
        git rebase -i HEAD~$1
    fi
    if [[ $STASHED_FIRST -eq 1 ]]
    then
        git stash pop
    fi
    echo "--------------------------------------------------"
    gst
}
